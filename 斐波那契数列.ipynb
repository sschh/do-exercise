   "source": [
    "### 递归时间复杂度过高 O(2^n)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# -*- coding:utf-8 -*-\n",
    "class Solution:\n",
    "    def Fibonacci(self, n):\n",
    "        state=[0,1]\n",
    "        for i in range(2,n+1):\n",
    "            state.append(state[i-1]+state[i-2])\n",
    "        return state[n]"
   ]
  },
  {
   "cell_type": "code",
